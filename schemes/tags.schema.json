{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://minecraft.com/tags.json",
    "type": "object",
    "title": "The Tags Schema",
    "required": [
      "replace",
      "values"
    ],
    "properties": {
      "replace": {
        "$id": "#/properties/replace",
        "type": "boolean",
        "title": "The Replace Schema",
        "default": false,
        "examples": [
          false
        ]
      },
      "values": {
        "$id": "#/properties/values",
        "type": "array",
        "title": "The Values Schema",
        "items": {
          "$id": "#/properties/values/items",
          "type": "string",
          "title": "The Items Schema",
          "default": "",
          "examples": [
            "minecraft:acacia_log"
          ],
          "pattern": "^minecraft:(.*)$"
        }
      }
    }
  }